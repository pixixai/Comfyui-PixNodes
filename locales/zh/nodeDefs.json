{
  "Pix_ImageSplitter": {
    "display_name": "图像切分 (Pix)",
    "description": "将输入的图像按指定的行列平均切分，并输出为图像批次。",
    "inputs": {
      "image": {
        "name": "图像",
        "tooltip": "输入的原始图像或图像批次"
      },
      "horizontal": {
        "name": "水平切分数",
        "tooltip": "水平方向要切分的列数"
      },
      "vertical": {
        "name": "垂直切分数",
        "tooltip": "垂直方向要切分的行数"
      }
    },
    "outputs": {
      "0": {
        "name": "图像批次",
        "tooltip": "切分后按顺序排列的图像批次结果"
      }
    }
  },
    "Pix_AnyDataIsEmpty": {
    "display_name": "是否空值",
    "description": "多功能数据校验节点，支持检查图片、潜空间、文本、列表等任何数据是否为空，常用于条件分支逻辑控制。",
    "inputs": {
      "invert": {
        "name": "反转逻辑",
        "tooltip": "默认（False）：空数据返回 True；开启（True）：空数据返回 False，常用于‘有数据才执行’的判断。"
      },
      "any_data": {
        "name": "任意数据",
        "tooltip": "连接需要检查的数据，支持通配符输入，可接受图片、文本、Latent 等任何类型。"
      }
    },
    "outputs": {
      "0": {
        "name": "布尔",
        "tooltip": "输出布尔值结果。若数据为空（如空字符串、空图片、None），则根据反转开关输出对应布尔值。"
      }
    }
  },
  "Pix_ColorPicker": {
    "display_name": "拾色器",
    "description": "支持 RGB、HSV 和 HEX 模式转换的颜色选择节点",
    "inputs": {
      "color_mode": {
        "name": "颜色模式",
        "tooltip": "选择输入颜色的表达方式",
        "options": {
          "RGB": "RGB 模式 (0-255)",
          "HSV": "HSV 模式 (色相/饱和度/明度)",
          "HEX": "HEX 模式 (16进制)"
        }
      },
      "value": {
        "name": "颜色值",
        "tooltip": "根据所选模式输入的具体颜色数值"
      }
    },
    "outputs": {
      "0": { "name": "RGB 色值", "tooltip": "输出格式为 'R,G,B' 的字符串" },
      "1": { "name": "RGB 整数", "tooltip": "输出 RGB 颜色的整数值" },
      "2": { "name": "HSV 色值", "tooltip": "输出格式为 'H,S,V' 的字符串" },
      "3": { "name": "HEX 值", "tooltip": "输出带 # 号的 16 进制颜色码" },
      "4": { "name": "HEX 值 (纯净版)", "tooltip": "输出不带 # 号的 16 进制颜色码" }
    }
  },
  "Pix_IfElseDispatcher": {
    "display_name": "IfElse 分发器",
    "description": "根据布尔条件将输入流分发至不同分支",
    "inputs": {
      "any_value": { "name": "输入值", "tooltip": "待分发的任意数据" },
      "if_true": { "name": "是否为真", "tooltip": "控制信号" }
    },
    "outputs": {
      "0": { "name": "真 (True)", "tooltip": "条件满足时的输出" },
      "1": { "name": "假 (False)", "tooltip": "条件不满足时的输出" }
    }
  },
  "Pix_IfElseLogicGate": {
    "display_name": "IfElse 逻辑阀",
    "description": "从两个分支中拾取有效的信号进行合并输出",
    "inputs": {
      "true_path": { "name": "真", "tooltip": "连接真分支信号" },
      "false_path": { "name": "假", "tooltip": "连接假分支信号" }
    },
    "outputs": {
      "0": { "name": "合并输出", "tooltip": "输出有效的信号值" }
    }
  },
  "Pix_Compare": {
    "display_name": "逻辑比较器",
    "description": "对两个输入进行数学或逻辑比较",
    "inputs": {
      "a": { "name": "输入 A", "tooltip": "左侧对象" },
      "b": { "name": "输入 B", "tooltip": "右侧对象" },
      "comparison": {
        "name": "比较条件",
        "tooltip": "选择比较逻辑",
        "options": {
          "a == b": "等于 (==)",
          "a != b": "不等于 (!=)",
          "a < b": "小于 (<)",
          "a > b": "大于 (>)",
          "a <= b": "小于等于 (<=)",
          "a >= b": "大于等于 (>=)"
        }
      }
    },
    "outputs": {
      "0": { "name": "布尔", "tooltip": "返回 True 或 False" }
    }
  },
  "Pix_MathInt": {
    "display_name": "整数运算器",
    "description": "执行基础的整数数学运算，如加减乘除",
    "inputs": {
      "a": { "name": "整数 A", "tooltip": "第一个运算数" },
      "b": { "name": "整数 B", "tooltip": "第二个运算数" },
      "operation": {
        "name": "运算操作",
        "tooltip": "选择要执行的数学算法",
        "options": {
          "add": "加法 (+)",
          "subtract": "减法 (-)",
          "multiply": "乘法 (×)",
          "divide": "整除 (//)",
          "modulo": "取模 (%)",
          "power": "幂运算 (^)"
        }
      }
    },
    "outputs": {
      "0": { "name": "运算结果", "tooltip": "输出计算后的整数值" }
    }
  },
    "Pix_ListToImageBatch": {
